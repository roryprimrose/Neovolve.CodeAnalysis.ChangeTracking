<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <NeutralLanguage>en-US</NeutralLanguage>	
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>

    <Title>Contract comparer for C# projects</Title>
    <Authors>rprimrose</Authors>
    <Description>This library provides contract comparison between versions of c# code.</Description>
    <PackageProjectUrl>https://github.com/roryprimrose/Neovolve.CodeAnalysis.ChangeTracking</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/roryprimrose/Neovolve.CodeAnalysis.ChangeTracking</RepositoryUrl>
    <PackageTags>Analysis</PackageTags>
    <Version>1.0.0-alpha</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ensure.That" Version="8.1.1" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.3.1" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="3.0.0" />
    <PackageReference Include="ModelBuilder" Version="5.0.0" />
    <PackageReference Include="nsubstitute" Version="4.2.1" />
  </ItemGroup>

</Project>
